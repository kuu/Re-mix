<table class="projects-table table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Stars</th>
      <th>Forks</th>
    </tr>
  </thead>
  <tbody>
  {{#each models}}
    <tr>
      <td><a href="/projects/{{owner}}/{{id}}">{{name}}</a></td>
      <td>{{starred.length}}</td>
      <td>{{forks.length}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
